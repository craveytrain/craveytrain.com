{
  "name": "craveytrain.com",
  "version": "7.0.0",
  "description": "The personal site of Mike Cravey (@craveytrain).",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf .cache public",
    "prebuild": "npm run clean",
    "build": "gatsby build",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --fix '*.js' 'src/**/*.js'",
    "lint:css": "npx stylelint 'src/**/*.{css,scss}' --fix",
    "precommit": "lint-staged",
    "format": "npm run format:all && npm run lint",
    "format:all": "prettier --write '*.{js,jsx,json,css,scss,md,yaml}!(package.lock.json)' 'src/**/*.{js,jsx,json,css,scss,md,yaml}'",
    "start": "gatsby develop",
    "upgrade-interactive": "npm-check --update"
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx}": [
        "prettier --write",
        "eslint --fix",
        "git add"
      ],
      "*.{css,scss}": [
        "prettier --write",
        "stylelint --fix",
        "git add"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/craveytrain/craveytrain.com.git"
  },
  "keywords": [
    "blog",
    "craveytrain"
  ],
  "author": "Mike Cravey <mike@craveytrain.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/craveytrain/craveytrain.com/issues"
  },
  "homepage": "https://github.com/craveytrain/craveytrain.com",
  "dependencies": {
    "ajv": "^6.10.0",
    "gatsby": "^2.9.4",
    "gatsby-plugin-catch-links": "^2.0.15",
    "gatsby-plugin-feed": "^2.2.3",
    "gatsby-plugin-google-analytics": "^2.0.21",
    "gatsby-plugin-manifest": "^2.1.1",
    "gatsby-plugin-netlify": "^2.0.17",
    "gatsby-plugin-offline": "^2.1.3",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-react-svg": "^2.1.1",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-plugin-typography": "^2.2.13",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-remark-autolink-headers": "^2.0.18",
    "gatsby-remark-prismjs": "^3.2.11",
    "gatsby-remark-smartypants": "^2.0.9",
    "gatsby-source-filesystem": "^2.0.39",
    "gatsby-transformer-remark": "^2.4.0",
    "lodash": "^4.17.11",
    "node-sass": "^4.12.0",
    "prismjs": "^1.16.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-typography": "^0.16.19",
    "typography": "^0.16.19",
    "typography-theme-sutro": "^0.16.19",
    "webpack": "^4.34.0"
  },
  "devDependencies": {
    "babel-plugin-module-resolver": "^3.2.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-jsx": "^6.0.2",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.1",
    "npm-check": "^5.9.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0"
  }
}
