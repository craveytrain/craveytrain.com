{
  "name": "craveytrain.com",
  "version": "7.0.0",
  "description": "The personal site of Mike Cravey (@craveytrain).",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf .cache public",
    "prebuild": "npm run clean",
    "build": "gatsby build",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "stylelint": "stylelint **/*.css",
    "stylelint:fix": "stylelint **/*.css --fix",
    "precommit": "lint-staged",
    "preformat": "npm run clean",
    "format": "prettier-standard '**/*.js'",
    "start": "gatsby develop",
    "upgrade-interactive": "npm-check --update"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/craveytrain/craveytrain.com.git"
  },
  "keywords": [
    "blog",
    "craveytrain"
  ],
  "author": "Mike Cravey <mike@craveytrain.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/craveytrain/craveytrain.com/issues"
  },
  "homepage": "https://github.com/craveytrain/craveytrain.com",
  "dependencies": {
    "ajv": "^6.5.3",
    "gatsby": "next",
    "gatsby-link": "next",
    "gatsby-plugin-catch-links": "next",
    "gatsby-plugin-feed": "next",
    "gatsby-plugin-google-analytics": "next",
    "gatsby-plugin-manifest": "next",
    "gatsby-plugin-netlify": "next",
    "gatsby-plugin-offline": "next",
    "gatsby-plugin-react-helmet": "next",
    "gatsby-plugin-react-svg": "next",
    "gatsby-plugin-resolve-src": "next",
    "gatsby-plugin-sass": "^2.0.0-rc.2",
    "gatsby-plugin-typography": "next",
    "gatsby-remark-autolink-headers": "next",
    "gatsby-remark-prismjs": "next",
    "gatsby-remark-smartypants": "next",
    "gatsby-source-filesystem": "next",
    "gatsby-transformer-remark": "next",
    "lodash.kebabcase": "^4.1.1",
    "node-sass": "^4.9.3",
    "prismjs": "^1.15.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-helmet": "^5.2.0",
    "react-typography": "^0.16.13",
    "typography": "^0.16.17",
    "typography-theme-sutro": "^0.15.10",
    "webpack": "^4.18.0"
  },
  "devDependencies": {
    "eslint": "^5.5.0",
    "eslint-plugin-graphql": "^2.1.1",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-json": "^1.2.1",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-markdown": "^1.0.0-beta.4",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "npm-check": "^5.8.0",
    "prettier-standard": "^8.0.1",
    "rimraf": "^2.6.2",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0"
  }
}
