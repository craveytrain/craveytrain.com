{
  "name": "craveytrain.com",
  "version": "7.0.0",
  "description": "The personal site of Mike Cravey (@craveytrain).",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf .cache public",
    "prebuild": "npm run clean",
    "build": "gatsby build",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --fix '*.js' 'src/**/*.js'",
    "lint:css": "npx stylelint 'src/**/*.{css,scss}' --fix",
    "precommit": "lint-staged",
    "format": "npm run format:all && npm run lint",
    "format:all": "prettier --write '*.{js,jsx,json,css,scss,md,yaml}!(package.lock.json)' 'src/**/*.{js,jsx,json,css,scss,md,yaml}'",
    "start": "gatsby develop",
    "upgrade-interactive": "npm-check --update"
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx}": [
        "prettier --write",
        "eslint --fix",
        "git add"
      ],
      "*.{css,scss}": [
        "prettier --write",
        "stylelint --fix",
        "git add"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/craveytrain/craveytrain.com.git"
  },
  "keywords": [
    "blog",
    "craveytrain"
  ],
  "author": "Mike Cravey <mike@craveytrain.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/craveytrain/craveytrain.com/issues"
  },
  "homepage": "https://github.com/craveytrain/craveytrain.com",
  "dependencies": {
    "ajv": "^6.5.3",
    "gatsby": "^2.0.0-rc.23",
    "gatsby-plugin-catch-links": "^2.0.2-rc.1",
    "gatsby-plugin-feed": "^2.0.0-rc.2",
    "gatsby-plugin-google-analytics": "^2.0.0-rc.2",
    "gatsby-plugin-manifest": "^2.0.2-rc.1",
    "gatsby-plugin-netlify": "^2.0.0-rc.6",
    "gatsby-plugin-offline": "^2.0.0-rc.7",
    "gatsby-plugin-react-helmet": "^3.0.0-rc.1",
    "gatsby-plugin-react-svg": "^2.0.0-beta1",
    "gatsby-plugin-sass": "^2.0.0-rc.2",
    "gatsby-plugin-typography": "^2.2.0-rc.3",
    "gatsby-remark-autolink-headers": "^2.0.0-rc.1",
    "gatsby-remark-prismjs": "^3.0.0-rc.2",
    "gatsby-remark-smartypants": "^2.0.0-rc.1",
    "gatsby-source-filesystem": "^2.0.1-rc.6",
    "gatsby-transformer-remark": "^2.1.1-rc.5",
    "lodash": "^4.17.11",
    "node-sass": "^4.9.3",
    "prismjs": "^1.15.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "react-typography": "^0.16.13",
    "typography": "^0.16.17",
    "typography-theme-sutro": "^0.15.10",
    "webpack": "^4.18.1"
  },
  "devDependencies": {
    "babel-plugin-module-resolver": "^3.1.1",
    "eslint": "^5.5.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-jsx": "^6.0.2",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "npm-check": "^5.8.0",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0"
  }
}
